<h1>show main controller </h1>
<% @medication.each do |medication| %>
  <p><%= link_to " #{ medication.name} (#{ medication.dosage })", medication_path(medication)%></p>
  <p><%= link_to "edit medication", edit_medication_path(medication) %>
  <%= link_to "delete medication",medication_path(medication), method: :delete %></p>
<% end %>
<%= link_to "Back to Reminders!", "/",class:"btn btn-primary"%>
